



if (input[row][col].getZ() < 5 && input[row][col].getZ() > 0) {
                    vertex.type = '0'; //its grass
                } else if ((input[row][col].getZ() > 5 && input[row][col].getZ() < 7) || input[row][col].getZ() < 0) {
                    vertex.type = 'X'; //its obstacle/water
                } else if (input[row][col].getZ() > 7 && input[row][col].getZ() < 8) {
                    vertex.type = 'S';//start
                    startVertex = vertex;
                } else {
                    vertex.type = 'H'; //hole
                    holeVertex = vertex;
                }



                shoot in hole direction
                divide the vx and vy by magnitude
                times const

//for maze bot

x0 = -45
y0 = -55
xt = 0
yt = 0
r = 0.05
muk = 0.1
mus = 0.2
heightProfile = 0
sandPitX = -1<x<1
sandPitY = 1<y<2
muks = 0.25
muss = 0.3

//for other bots

x0 = -3
y0 = 0
xt = 4
yt = 1
r = 0.05
